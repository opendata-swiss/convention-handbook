:DatasetShape
  rdf:type sh:NodeShape ;
  sh:targetClass dcat:Dataset ;
  sh:property [
      sh:path dct:title ;
      sh:minCount 1 ;
      sh:nodeKind sh:Literal ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dct:description ;
      sh:minCount 1 ;
      sh:nodeKind sh:Literal ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dct:issued ;
      sh:maxCount 1 ;
      sh:severity sh:Violation ;
      sh:node :DateType ;
    ] ;
  sh:property [
      sh:path dct:modified ;
      sh:maxCount 1 ;
      sh:severity sh:Violation ;
      sh:node :DateType ;
    ] ;
  sh:property [
      sh:path dct:publisher ;
      sh:class foaf:Agent ;
      sh:minCount 1 ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dcat:contactPoint ;
      sh:class vcard:Kind ;
      sh:minCount 1 ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dct:language ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dcat:theme ;
      sh:class skos:Concept ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dct:distribution ;
      sh:class dct:Distribution ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dcat:keyword ;
      sh:nodeKind sh:Literal ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dcat:landingPage ;
      sh:class foaf:Document ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dct:spatial ;
      sh:class dct:Location ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dct:coverage ;
      sh:class dct:LocationPeriodOrJurisdiction ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dct:temporal ;
      sh:class dct:PeriodOfTime ;
            sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dct:accrualPeriodicity ;
      sh:maxCount 1 ;
            sh:class dct:Frequency ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dct:identifier ;
      sh:nodeKind sh:Literal ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
            sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dct:relation ;
      sh:class rdfs:Resource ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path rdfs:seeAlso ;
      sh:class rdfs:Resource ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path schema:image ;
      sh:maxCount 3 ;
      sh:severity sh:Violation ;
      sh:or (
            [
                sh:class schema:URL ;
            ]
            [
                sh:class schema:ImageObject ;
            ]
        )
    ] ;
  sh:property [
      sh:path adms:version ;
      sh:maxCount 0 ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path adms:versionNotes ;
      sh:maxCount 0 ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path adms:identifier ;
      sh:maxCount 0 ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path dct:conformsTo ;
      sh:maxCount 0 ;
      sh:severity sh:Violation ;
    ] ;
.
